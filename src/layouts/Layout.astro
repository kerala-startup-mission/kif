		<title>Kerala Innovation Festival | Kerala Startup Mission</title>

		<!--=====FAB ICON=======-->
		<link rel="shortcut icon" href="/favicon.png" />

		<!--===== CSS LINK =======-->
		<style is:global>
			@keyframes float {
			  0%, 100% {
				transform: translateY(0);
			  }
			  50% {
				transform: translateY(-10px);
			  }
			}
		  
			@keyframes float-in {
			  from { transform: translateY(50px); opacity: 1; }
			  to { transform: translateY(0); opacity: 1; }
			}
		  
			@keyframes fall-in {
			  0% { transform: translateY(-200px); opacity: 1; }
			  60% { transform: translateY(20px); opacity: 1; }
			  100% { transform: translateY(0); }
			}
		  
			@keyframes rotate-in {
			  from { transform: rotate(0deg); opacity: 1; }
			  to { transform: rotate(3.5deg); opacity: 1; }
			}
		  
			.animate-float {
			  animation: float 3s ease-in-out infinite;
			}
		  
			.animate-float-in {
			  animation: float-in 1s ease-out forwards;
			}
		  
			.animate-fall-in {
			  animation: fall-in 1.2s ease-out forwards;
			}
		  
			.animate-rotate-in {
			  animation: rotate-in 0.6s ease-out forwards;
			}
		</style>
